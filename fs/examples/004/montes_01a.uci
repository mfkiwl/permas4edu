NEW

INPUT
    READ PERMAS FILE = montes_01a.dat
RETURN

TASK
EXEC 
    ACT SIT = STEP_1
    VIBRATION ANALYSIS MODES = 10
EXPORT
    ITEM MODL
    GO PERMAS BINARY FILE = s_modes
EXEC
    ACT SIT = STEP_2
    FLUID VIBRATION MODES = 10
EXPORT
    ITEM MODL
    GO PERMAS BINARY FILE = f_modes
EXEC
    ACT SIT = STEP_3
    FSCOUPLED VIBRATION ANALYSIS MODES = 10
EXPORT
    ITEM MODL
    GO PERMAS BINARY FILE = fs_modes
TASK END
STOP
