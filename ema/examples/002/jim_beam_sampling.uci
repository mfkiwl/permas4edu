NEW

INPUT
    READ PERMAS FILE = jim_beam.dat
    READ PERMAS FILE = jim_beam_add.dat
    READ PERMAS FILE = jim_beam_sampling.dat
RETURN

TASK LOOPS = 9
EXEC
    ACT SIT = STEP_1
    VIBRATION ANALYSIS MODES = 10
    ACT SIT = SAMPLING
    SAMPLING
    GEN EPRES
EXPORT
    ITEM CLEAR
    CURRENT COLUMNS = 1
    ITEM EPRES
TASK END
EXPORT
    ITEM CLEAR
    ITEM XYDATA TYPE = DHIS
    ITEM XYDATA TYPE = SRHIS
    GO SPREADSHEET FILE = sampling
STOP
