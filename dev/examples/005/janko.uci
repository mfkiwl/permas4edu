NEW

INPUT
    READ PERMAS FILE = janko_model.dat
    READ PERMAS FILE = janko.dat
RETURN

TASK
EXEC
    ACT SIT = STEP_1
    VIBRATION ANALYSIS MODES = 10
    GEN STRAIN ENERGY
PRINT
    PORT RES REPLACE FILE = natural_frequencies.txt
    TEXT = 'ALUMINUM'
    DATA OBJECT RVALTAB.DISP
    PORT RES RESET
EXPORT 
    ITEM MODL
    GO PERMAS BINARY FILE = normal_modes_aluminum
EXEC
    ACT SIT = STEP_2
    VIBRATION ANALYSIS MODES = 10
    GEN STRAIN ENERGY
    GEN MAC SITUATION = STEP_1
PRINT
    PORT RES SWITCH TO FILE = natural_frequencies.txt
    TEXT = 'STEEL'
    DATA OBJECT RVALTAB.DISP
    PORT RES RESET
EXPORT 
    OPEN PERMAS ASCII FILE = mac
    DATA OBJECT MAC.DISP BLOCKFORM = VALUE
    ITEM MODL
    GO PERMAS BINARY FILE = normal_modes_steel
TASK END
USER
    CALL TOOL6 P1 = 'pyINTES mac_mod.py mac.pmat natural_frequencies.txt'
STOP
