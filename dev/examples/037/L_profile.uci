SET MASSNORM = ON ! Export mass normalized modes

NEW

INPUT
    READ PERMAS FILE = L_profile.dat
    READ PERMAS FILE = L_profile_add.dat
RETURN

TASK
EXEC
    ACT SIT = STEP_1
    VIBRATION ANALYSIS MODES = 30
PRINT
    PORT RES REPLACE FILE = natural_frequencies.txt
    DATA OBJECT RVALTAB.DISP
    PORT RES RESET
EXPORT
    ITEM MODL
    GO PERMAS BINARY FILE = normal_modes
TASK END
STOP
