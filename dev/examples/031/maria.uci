NEW

INPUT
    READ PERMAS FILE = maria.dat
    READ PERMAS FILE = maria_add.dat
RETURN

TASK
EXEC
    ACT SIT = STEP_1
    VIBRATION ANALYSIS MODES = 20
PRINT 
    PORT RES REPLACE FILE = natural_frequencies.txt
    DATA OBJECT RVALTAB.DISP
    PORT RES RESET
SELECT
    ACTIVE NSET = ALL_NODES
    CURRENT COLUMNS = 1, 7
    XYDATA ITEM DISP COOR = Z RESULT = MODE DCOMP = 2
EXPORT
    ITEM CLEAR XYDATA
    ITEM MODL
    GO PERMAS BINARY FILE = normal_modes
    ITEM CLEAR
    ITEM XYDATA NAME = MODE
    GO SPREADSHEET FILE = normal_modes
TASK END
USER 
    CALL TOOL6 P1 = 'pyINTES maria.py normal_modes_mode.csv'
RETURN
STOP