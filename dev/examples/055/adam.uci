NEW

INPUT
    READ PERMAS FILE = adam.dat
    READ PERMAS FILE = adam_add.dat
RETURN

TASK
EXEC
    ACT SIT = STEP_1
    VIBRATION ANALYSIS MODES = 18 FREQLIM = 1000.0
PRINT
    PORT RES REPLACE FILE = natural_frequencies.txt
    DATA OBJECT RVALTAB.DISP
    PORT RES RESET
EXPORT
    ITEM MODL
    GO PERMAS BINARY FILE = normal_modes
!
!   Excitation @A
!
EXEC
    MODAL FREQUENCY SITUATION = STEP_2
    ACT SIT = STEP_2
SELECT
    ACTIVE NSET = ALL_NODES
    XYDATA ITEM DISP RESULT = RESPA DCOMP = 3
EXPORT
    ITEM CLEAR
    PARAM SPREADCURVES = 1005
    ITEM XYDATA NAME = RESPA
    GO SPREADSHEET FILE = modal_frf
!
!   Excitation @B
!
EXEC
    MODAL FREQUENCY SITUATION = STEP_3
    ACT SIT = STEP_3
SELECT
    ACTIVE NSET = ALL_NODES
    XYDATA ITEM DISP RESULT = RESPB DCOMP = 3
EXPORT
    ITEM CLEAR
    PARAM SPREADCURVES = 1005
    ITEM XYDATA NAME = RESPB
    GO SPREADSHEET FILE = modal_frf
TASK END
STOP
