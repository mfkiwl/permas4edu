NEW

USER
    CALL TOOL6 P1 = 'pyINTES varying_thickness.py'
RETURN

INPUT
    READ PERMAS FILE = circular_plate.dat
    READ PERMAS FILE = thickness.dat  ! generated by previously called Python script
    READ PERMAS FILE = sampling.dat
RETURN

TASK LOOPS = 1
EXEC
    ACT SIT = STEP_1
    VIBRATION ANALYSIS MODES = 20
EXPORT
    ITEM MODL
    GO PERMAS BINARY FILE = normal_modes
PRINT
    PORT RES REPLACE FILE = natural_frequencies.txt
    DATA OBJECT RVALTAB.DISP
    PORT RES RESET
EXEC
    ACT SIT = SAMPLING
    SAMPLING
TASK END
EXPORT
    ITEM CLEAR
    ITEM XYDATA TYPE = DHIS
    ITEM XYDATA TYPE = SRHIS
    GO SPREADSHEET FILE = sampling
STOP
