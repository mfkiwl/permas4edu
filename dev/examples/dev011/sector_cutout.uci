NEW

INPUT
    READ PERMAS FILE = sector_cutout.dat
RETURN

TASK 
EXEC
    ACT SIT = STEP_1
    VIBRATION ANALYSIS MODES = 6
PRINT
    PORT RES REPLACE FILE = natural_frequencies.txt
    TEXT = 'FFFF'
    DATA OBJECT RVALTAB.DISP
    PORT RES RESET
EXPORT
    OPEN PERMAS ASCII FILE = natural_frequencies
    DATA OBJECT RVALTAB.DISP
    ITEM MODL
    GO PERMAS BINARY FILE = normal_modes_ffff
TASK END
USER
    CALL TOOL6 P1 = 'pyINTES sector_cutout.py natural_frequencies.pmat' ! Non-dimensional frequencies
RETURN

