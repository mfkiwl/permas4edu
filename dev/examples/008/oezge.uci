NEW

INPUT
    READ PERMAS FILE = oezge.dat
RETURN

TASK
EXEC
    ACT SIT = STEP_1
    VIBRATION ANALYSIS MODES = 5
SELECT
    ACTIVE NSET = ALL_NODES
    XYDATA ITEM DISP COOR = X RESULT = MODES DCOMP = 1
EXPORT
    ITEM CLEAR
    ITEM XYDATA NAME = MODES
    GO SPREADSHEET FILE = modes
    ITEM CLEAR
    ITEM MODL 
    ITEM DISP
    GO PERMAS BINARY FILE = normal_modes
EXEC
    MODAL COMPLEX SITUATION = STEP_2
    ACT SIT = STEP_2
EXPORT
    ITEM CLEAR
    ITEM MODL
    ITEM DISP 
    GO PERMAS BINARY FILE = complex_modes
TASK END
USER
    CALL TOOL6 P1 = 'pyINTES oezge.py modes_modes.csv'
STOP

    
