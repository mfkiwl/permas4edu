$ENTER COMPONENT  NAME = DFLT_COMP  DOFTYPE = DISP MATH
!------
   $SITUATION  NAME = STEP_1
      DFLT_COMP  CONSTRAINTS = C_STEP_1  SYSTEM = S_STEP_1
   $END SITUATION
!
   $STRUCTURE
!
      $ELSYS RSYS
       COLUMNS        RSYS =          0  AXES =  1,  2
!
   $END STRUCTURE
!------
   $CONSTRAINTS  NAME = C_STEP_1
!
      $SUPPRESS  DOFS = 1,2,3,4,5,6  DOFTYPE = DISP
       GROUND
!
   $END CONSTRAINTS
!------
   $SYSTEM  NAME = S_STEP_1
!
      $MODDAMP TYPE = RATIO KIND = FUNCTION
        0.01
!
      $GEODAT SHELL CONT = THICK NODES = ALL
       GD_PLATE 5.0
!
      $GEODAT BEAM      CONT = SECTTYPE SECTDIM  SECTDIM
       GD_COLUMN             5 :  5.000000E+00  5.000000E+00 :  5.000000E+00  5.000000E+00
!
      $ELPROP
       PLATE         GEODAT = GD_PLATE      MATERIAL = STEEL
       COLUMNS       GEODAT = GD_COLUMN     MATERIAL = STEEL
!
   $END SYSTEM
!------
$EXIT COMPONENT
!------------------------------------
$ENTER MATERIAL
!------
   $MATERIAL  NAME = ALUMINUM  TYPE = ISO
!
      $DENSITY  GENERAL  INPUT = DATA
        2.76000E-09
!
      $ELASTIC  GENERAL  INPUT = DATA
        7.000000E+04  3.000000E-01
!
   $END MATERIAL
!------
   $MATERIAL  NAME = STEEL  TYPE = ISO
!
      $DENSITY  GENERAL  INPUT = DATA
        7.85000E-09
!
      $ELASTIC  GENERAL  INPUT = DATA
        2.100000E+05  3.000000E-01
!
   $END MATERIAL
!------
$EXIT MATERIAL
!------------------------------------
$FIN
