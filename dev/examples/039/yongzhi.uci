NEW

INPUT
    READ PERMAS FILE = yongzhi.dat
    READ PERMAS FILE = yongzhi_add.dat
RETURN

TASK
EXEC
    ACT SIT = STEP_1
    VIBRATION ANALYSIS MODES = 10
SELECT
    ACTIVE NSET = ALL_NODES
    XYDATA ITEM DISP COOR = X RESULT = MODE DCOMP = 2
PRINT
    PORT RES REPLACE FILE = natural_frequencies.txt
    DATA OBJECT RVALTAB.DISP
    PORT RES RESET
EXPORT
    ITEM CLEAR XYDATA
    ITEM MODL
    GO PERMAS BINARY FILE = normal_modes
    ITEM CLEAR
    ITEM XYDATA NAME = MODE
    GO SPREADSHEET 
TASK END
USER
    CALL TOOL6 P1 = 'pyINTES yongzhi.py'
RETURN
STOP
