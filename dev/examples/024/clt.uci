NEW

INPUT
    READ PERMAS FILE = clt.dat
    READ PERMAS FILE = clt_add.dat
RETURN

TASK
EXEC
    ACT SIT = STEP_1
    VIBRATION ANALYSIS MODES = 50
PRINT
    PORT RES REPLACE FILE = natural_frequencies.txt
    DATA OBJECT RVALTAB.DISP
    PORT RES RESET
EXPORT
    ITEM MODL
    GO PERMAS BINARY FILE = normal_modes
EXEC
    MODAL FREQUENCY SITUATION = STEP_2
    ACT SIT = STEP_2
SELECT
    ACTIVE NSET = RESPONSE
    XYDATA ITEM DISP BOOK = USR  RESULT = AMPL  DCOMP = 3
    XYDATA ITEM DISP BOOK = USRF RESULT = PHASE DCOMP = 3
EXPORT
    ITEM CLEAR
    ITEM XYDATA NAME = AMPL
    GO SPREADSHEET FILE = frf_01
    ITEM CLEAR 
    ITEM XYDATA NAME = PHASE
    GO SPREADSHEET FILE = frf_02
TASK END
USER
    CALL TOOL6 P1 = 'pyINTES clt.py'
STOP