NEW

INPUT
    READ PERMAS FILE = sector_fine.dat
    READ PERMAS FILE = sector_fine_add.dat
    READ PERMAS FILE = sector_fine_sampling.dat
RETURN

TASK LOOPS = 1
EXEC
    ACT SIT = STEP_1
    VIBRATION ANALYSIS MODES = 10
EXPORT
