NEW

INPUT
    READ PERMAS FILE = qiang.dat
RETURN

TASK
EXEC
    ACT SIT = STEP_1
    VIBRATION ANALYSIS MODES = 8
PRINT
    PORT RES REPLACE FILE = natural_frequencies.txt
    DATA OBJECT RVALTAB.DISP
    PORT RES RESET
SELECT
    ACTIVE NSET = ALL_NODES
    XYDATA ITEM DISP COOR = Y   RESULT = MODES  DCOMP = 2
EXPORT
    ITEM CLEAR XYDATA 
    ITEM MODL
    GO PERMAS BINARY FILE = normal_modes
    ITEM CLEAR
    ITEM XYDATA NAME = MODES
    GO SPREADSHEET FILE = modes
TASK END
USER
    CALL TOOL6 P1 = 'pyINTES qiang.py'
RETURN
STOP