NEW

INPUT
    READ PERMAS FILE = natalia_01.dat
    READ PERMAS FILE = natalia_01_add.dat
RETURN

TASK
EXEC
    ACT SIT = STEP_1
    VIBRATION ANALYSIS MODES = 7
PRINT
    PORT RES REPLACE FILE = natural_frequencies.txt
    TEXT = 'FE Fixed'
    DATA OBJECT RVALTAB.DISP
    PORT RES RESET
EXPORT
    ITEM MODL
    GO PERMAS BINARY FILE = normal_modes_1
EXEC
    ACT SIT = STEP_2
    VIBRATION ANALYSIS MODES = 7
    GEN MAC SITUATION = STEP_1
PRINT
    PORT  RES SWITCH TO FILE = natural_frequencies.txt
    TEXT = 'FE Spring'
    DATA OBJECT RVALTAB.DISP
    PORT RES RESET
EXPORT
    OPEN PERMAS ASCII FILE = mac_fixed_free
    DATA OBJECT MAC.DISP BLOCKFORM = VALUE    
    ITEM MODL
    GO PERMAS BINARY FILE = normal_modes_2
TASK END
STOP

