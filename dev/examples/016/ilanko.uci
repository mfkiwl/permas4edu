NEW

INPUT
    READ PERMAS FILE = ilanko.dat
    READ PERMAS FILE = ilanko_add.dat
    READ PERMAS FILE = ilanko_position.dat
RETURN

PORT RES REPLACE FILE = natural_frequencies.txt
PORT RES RESET

TASK LOOPS = 3
EXEC
    ACT SIT = STEP_1
    VIBRATION ANALYSIS MODES = 10
PRINT
    PORT RES SWITCH TO FILE = natural_frequencies.txt
    TEXT = 'Task loop #%(tloop)'
    DATA OBJECT RVALTAB.DISP
    PORT RES RESET
EXPORT
    ITEM MODL NEWCOOR
    GO PERMAS BINARY FILE = 'normal_modes_%(tloop)'
EXEC
    ACT SIT = SAMPLING
    SAMPLING
TASK END
EXPORT
    ITEM CLEAR
    ITEM XYDATA TYPE = DHIS
    ITEM XYDATA TYPE = SRHIS
    GO SPREADSHEET FILE = sampling
STOP
