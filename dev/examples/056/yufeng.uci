NEW

INPUT
    READ PERMAS FILE = yufeng.dat
    READ PERMAS FILE = yufeng_add.dat
    READ PERMAS FILE = yufeng_sampling.dat
RETURN

TASK LOOPS = 1
EXEC
    ACT SIT = STEP_1
    VIBRATION ANALYSIS MODES = 30
EXPORT
