NEW

INPUT
    READ PERMAS FILE = robin_01.dat
    READ PERMAS FILE = robin_01_sampling.dat
RETURN

TASK LOOPS = 100
EXEC
    ACT SIT = STEP_1
    VIBRATION ANALYSIS MODES = 4
    ACT SIT = SAMPLING
    SAMPLING METHOD = LHC NSAMPLES = 100
TASK END
EXPORT
    ITEM CLEAR
    ITEM XYDATA TYPE = DHIS
    ITEM XYDATA TYPE = SRHIS
    GO SPREADSHEET FILE = sampling 
STOP
